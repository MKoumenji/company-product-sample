{
  "ConnectionStrings": {
    "CurrentDB": "***"
  },

  "IisConfig": {
    "SiteName": "***",
    "PoolName": "***",
    "WaitingTimeStartInSec": "10",
    "WaitingTimeStopInSec": "10"
  },

  "ApplicationSettings": {
    "TimeSpanInMinutes": 1440,
    "OrderProcessingTimeInSec": 5,
    "MarginInMinutes":5,
    "OrderHandlerUrl": "http://Test/OrderHandler/v1/orders",
    "ExecutionDelayInMinutes": 30,
    "StartMinute": 15
  },

  "MailKitConfig": {
    "ServiceName": "Prod: company.product",
    "SmtpServer": "**",
    "SmtpPort": 25,
    "SmtpUsername": "**",
    "SmtpPassword": "**",
    "EmailFrom": "**",
    "EmailTo": "**"
  },

  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FomLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "company.product"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Log\\company.product\\LogFile.txt",
          "rollingInterval": "Hour"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://HAM:1234/",
          "apiKey": "****"
        }
      }
    ]
  }
}