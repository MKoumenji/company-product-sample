{
  "ConnectionStrings": {
    "TestDB": "Server=server;Database=db;Trusted_Connection=True;MultipleActiveResultSets=true",
    "TestDB_": "Server=server;Database=db;Trusted_Connection=True;MultipleActiveResultSets=true",
    "NoDB": "Server=xxxx;Database=XXXXX;Trusted_Connection=True;MultipleActiveResultSets=true"
  },

  "IisConfig": {
    "SiteName": "OrderHandler",
    "PoolName": "OrderHandler",
    "WaitingTimeStartInSec": "10",
    "WaitingTimeStopInSec": "10"
  },


  "OrchestratorSettings": {
    "TimeSpanInMinutes": 120,
    "OrderProcessingTimeInSec": 5,
    "OrderHandlerUrl": "http://server:port/OrderHandler/v1/orders"
  },
  "MailKitConfig": {
    "ServiceName": "Staging: company.product",
    "SmtpServer": "mail.server",
    "SmtpPort": 25,
    "SmtpUsername": "",
    "SmtpPassword": "",
    "EmailFrom": "",
    "EmailTo": ""
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FomLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "\\Log\\company.product\\LogFile.txt",
          "rollingInterval": "Hour"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "url",
          "apiKey": ""
        }
      }
    ]
  }
}