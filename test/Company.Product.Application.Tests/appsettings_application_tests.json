{
  "ConnectionStrings": {
    "TestDB": "Server=xxxx;Database=XXXXX;Trusted_Connection=True;MultipleActiveResultSets=true"
  
  },

  "IisConfig": {
    "SiteName": "compyany.product.test",
    "WaitingTimeStartInSec": "3",
    "WaitingTimeStopInSec": "3"
  },
  
  "ApplicationSettings": {
    "TimeSpanInMinutes": 1440,
    "OrderProcessingTimeInSec": 5,
    "MarginInMinutes":5,
    "OrderHandlerUrl": "http://Test/company.product/v1/orders",
    "ExecutionDelayInMinutes": 30,
    "StartMinute": 15
  },

  
  "MailKitConfig": {
    "ServiceName": "Staging: company.product",
    "SmtpServer": "mail.server",
    "SmtpPort": 25,
    "SmtpUsername": "",
    "SmtpPassword": "",
    "EmailFrom": "",
    "EmailTo": ""
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FomLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "company.product"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "\\Log\\company.product\\LogFile.txt",
          "rollingInterval": "Hour"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "",
          "apiKey": ""
        }
      }
    ]
  }
}